{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "adamtyn/lumen-artisan-storage-link": "^1.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "flipbox/lumen-generator": "^8.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/cloud-firestore": "^1.19",
        "grpc/grpc": "^1.38",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-id-generator": "^1.0",
        "illuminate/mail": "^7",
        "illuminate/notifications": "^7",
        "illuminate/redis": "^7",
        "illuminate/support": "^7",
        "kreait/laravel-firebase": "3.0",
        "laravel/lumen-framework": "^7.0",
        "laravelista/lumen-vendor-publish": "^7.0",
        "maatwebsite/excel": "^3.1",
        "overtrue/laravel-like": "^4.0",
        "predis/predis": "~1.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-json-api-paginate": "^1.10",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-query-builder": "^3.3",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^4.4",
        "knuckleswtf/scribe": "^2.5",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
